{
  "name": "bookamot-saas",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/build-sw.js",
    "build": "next build",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "start": "prisma migrate deploy && next start -p ${PORT:-3000}",
    "lint": "next lint",
    "db": "tsx scripts/db.ts",
    "db:clean": "tsx scripts/db.ts clean",
    "db:clean:secure": "tsx scripts/clean-database.ts",
    "db:clean:dry-run": "tsx scripts/clean-database.ts --dry-run --verbose",
    "db:reset": "tsx scripts/db.ts reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:list": "tsx scripts/db.ts list",
    "db:add-email-columns": "node scripts/add-email-verification-columns.js",
    "booking:complete": "tsx scripts/booking.ts complete",
    "query:users": "tsx scripts/query.ts users",
    "query:garages": "tsx scripts/query.ts garages",
    "query:slots": "tsx scripts/query.ts slots",
    "verify:deployment": "bash scripts/verify-deployment.sh",
    "setup:test-data": "bash scripts/setup-test-data.sh",
    "test:refresh-loop": "tsx scripts/test-refresh-loop.ts",
    "test:lookup": "tsx scripts/test-vehicle-lookup.ts",
    "test:dvsa": "tsx scripts/test-dvsa-oauth.ts",
    "test:mot-errors": "tsx scripts/test-mot-history-errors.ts",
    "test:mot-rate": "tsx scripts/test-mot-history-rate-limit.ts",
    "test:mot-mock": "tsx scripts/test-mot-history-integration-mock.ts",
    "test:mot-mapping": "tsx scripts/test-mot-mapping.ts",
    "test:api-vehicle": "DATABASE_URL=postgresql://bookamot:changeme@localhost:5432/bookamot tsx scripts/test-api-vehicle-details.ts",
    "test:mot-db": "DATABASE_URL=postgresql://bookamot:changeme@localhost:5432/bookamot tsx scripts/test-mot-history-db-match.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.540.0",
    "next": "15.4.7",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.2.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.7",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
